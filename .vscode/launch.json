{
  "configurations": [
    {
      "name": "Client app",
      "request": "launch",
      "cwd": "${workspaceFolder}/demo_app/client",
      "runtimeArgs": [
        "start"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "java",
      "name": "Spring Boot-Application<demo>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "io.github.mucsi96.demo.Application",
      "projectName": "demo",
      "args": "--spring.profiles.active=local",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
