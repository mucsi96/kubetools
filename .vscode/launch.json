{
  "configurations": [
    {
      "name": "Client app",
      "request": "launch",
      "cwd": "${workspaceFolder}/demo_app/client",
      "runtimeArgs": ["start"],
      "runtimeExecutable": "yarn",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "java",
      "name": "Spring Boot-SpringBootAdminApplication<spring-boot-admin>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "io.github.mucsi96.SpringBootAdminApplication",
      "projectName": "spring-boot-admin",
      "args": "--spring.profiles.active=local",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "java",
      "name": "Spring Boot-DemoApplication<demo>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "io.github.mucsi96.DemoApplication",
      "projectName": "demo",
      "args": "--spring.profiles.active=local",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
