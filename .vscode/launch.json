{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-SpringBootAdminApplication<spring-boot-admin>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.example.demo.SpringBootAdminApplication",
            "projectName": "spring-boot-admin",
            "args": "--spring.profiles.active=local",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Spring Boot-DemoApplication<demo>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.example.demo.DemoApplication",
            "projectName": "demo",
            "args": "--spring.profiles.active=local",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}